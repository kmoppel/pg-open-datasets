T1=$(date +%s%3N) # Epoch millis
pg_dump -d $DATASET_NAME >/dev/null
T2=$(date +%s%3N)
DURATION=$((T2-T1))

SQL_INS=$(cat <<-EOF
INSERT INTO dataset_test_results (
    script_start, test_start, dataset_name, test_name, test_id, test_value
) VALUES (
    '${SCRIPT_START}', '${TEST_START}', '${DATASET_NAME}', '${TEST_NAME}', 'pg_dump_gzip_${LVL}', $DURATION
);
EOF
)
psql "$RDB_CONNSTR" -Xc "$SQL_INS"
